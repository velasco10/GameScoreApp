{
  "expo": {
    "name": "ScoreQuest",
    "slug": "scorequest",
    "version": "1.0.0",
    "entryPoint": "./app/index.tsx",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "scheme": "scorequest",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.velascode.scorequest",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "velascode",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c567d70d-cf70-48b3-a724-bf63fda4171b"
      }
    }
  }
}
